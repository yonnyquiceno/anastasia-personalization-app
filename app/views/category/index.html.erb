<h4>Mis referencias</h4>

<table class="table">
  <thead>
    <th>Id</th>
    <th>Referencia</th>
    <th>Imagen</th>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= category.id %></td>
        <td><%= category.category_name %></td>
        <td><%= category.image %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "Inicio", root_path %>
